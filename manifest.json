{
    "name": "E-Stamp Quick Fill",
    "version": "0.0.4",
    "manifest_version": 3,
    "description": "Quickly save & get data of e-stamp form.",
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "logo1.png"
    },
    "icons": {
        "16": "logo1.png",
        "48": "logo1.png",
        "128": "logo1.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.shcilestamp.com/eStampIndia/submission/SubmissionServlet"
            ],
            "js": [
                "popup.js"
            ]
        }
    ]
}